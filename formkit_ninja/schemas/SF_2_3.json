{
  "children": [
    {
      "children": [
        {
          "key": "district",
          "id": "district",
          "name": "district",
          "label": "$gettext(Municipality)",
          "validation": "required",
          "$formkit": "select",
          "options": "$getLocations()"
        },
        {
          "key": "administrative_post",
          "if": "$get(district).value",
          "id": "administrative_post",
          "name": "administrative_post",
          "label": "$gettext(\"Administrative Post\")",
          "validation": "required",
          "$formkit": "select",
          "options": "$getLocations($get(district).value)"
        },
        {
          "key": "suco",
          "if": "$get(administrative_post).value",
          "id": "suco",
          "name": "suco",
          "label": "$gettext(Suco)",
          "validation": "required",
          "$formkit": "select",
          "options": "$getLocations($get(district).value, $get(administrative_post).value)"
        },
        {
          "key": "date",
          "id": "date",
          "name": "date",
          "label": "$gettext(\"Date\")",
          "validation": "dateInPast",
          "$formkit": "datepicker",
          "calendarIcon": "calendar",
          "format": "DD/MM/YYYY",
          "nextIcon": "angleRight",
          "prevIcon": "angleLeft",
          "_currentDate": "$getCurrentDate",
          "sectionsSchema": {
            "day": {
              "children": [
                "$day.getDate()",
                {
                  "if": "$attrs._currentDate().day === $day.getDate()",
                  "children": [
                    {
                      "if": "$attrs._currentDate().month === $day.getMonth()",
                      "children": [
                        {
                          "if": "$attrs._currentDate().year === $day.getFullYear()",
                          "$el": "div",
                          "attrs": {
                            "class": "formkit-day-highlight"
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          },
          "valueFormat": "YYYY-MM-DD"
        }
      ],
      "id": "location",
      "$formkit": "group",
      "icon": "las la-map-marked-alt",
      "title": "Location"
    },
    {
      "children": [
        {
          "children": [
            {
              "children": [
                "$gettext(\"priority\")",
                " ",
                {
                  "children": [
                    "$: ($index + 1)"
                  ],
                  "$el": "span",
                  "attrs": {
                    "class": "ml-1"
                  }
                }
              ],
              "$el": "div",
              "attrs": {
                "class": "rounded-full px-5 py-2 bg-zinc-400 text-lg font-bold mb-5"
              }
            },
            {
              "name": "uuid",
              "$formkit": "uuid",
              "readonly": true
            },
            {
              "key": "aldeia",
              "name": "aldeia",
              "label": "Aldeia",
              "validation": "required",
              "placeholder": "$gettext(\"Select one\")",
              "$formkit": "select",
              "options": "$getLocations($get(district).value, $get(administrative_post).value, $get(suco).value)"
            },
            {
              "key": "project_sector",
              "id": "$: (project_sector + $index)",
              "name": "project_sector",
              "label": "$gettext(\"Project Sector\")",
              "validation": "required",
              "placeholder": "$gettext(\"Please select\")",
              "$formkit": "select",
              "options": "$ida(sector)"
            },
            {
              "key": "project_sub_sector",
              "if": "$get(project_sector + $index).value",
              "id": "$: (project_sub_sector + $index)",
              "name": "project_sub_sector",
              "label": "$gettext(\"Project Sub-Sector\")",
              "validation": "required",
              "placeholder": "$gettext(\"Please select\")",
              "$formkit": "select",
              "options": "$ida(subsector, \"sector_id=\"+$get(project_sector + $index).value)"
            },
            {
              "key": "project_name",
              "if": "$get(project_sub_sector + $index).value",
              "id": "$: (project_name + $index)",
              "name": "project_name",
              "label": "$gettext(\"Project name\")",
              "validation": "required",
              "placeholder": "$gettext(\"Please select\")",
              "$formkit": "select",
              "options": "$ida(output, \"subsector_id=\"+$get(project_sub_sector + $index).value)"
            },
            {
              "name": "place",
              "label": "$gettext(\"Place\")",
              "validation": "required",
              "$formkit": "textarea"
            },
            {
              "key": "unit_select",
              "if": "$get(project_name).value",
              "name": "unit",
              "label": "$pgettext('sf23_priority', 'Volume / Unit')",
              "validation": "required",
              "$formkit": "select",
              "options": "$ida(unit)"
            },
            {
              "key": "project_type",
              "name": "project_type",
              "label": "$gettext(\"Project type\")",
              "validation": "required",
              "placeholder": "$gettext(\"Please select\")",
              "$formkit": "select",
              "options": "$ida(activity)"
            },
            {
              "name": "beneficiaries_female",
              "label": "$gettext(\"No. of beneficiaries female\")",
              "validation": "required",
              "$formkit": "number",
              "min": 0
            },
            {
              "name": "beneficiaries_male",
              "label": "$gettext(\"No. of beneficiaries male\")",
              "validation": "required",
              "$formkit": "number",
              "min": 0
            },
            {
              "name": "households",
              "label": "$gettext(\"No. of households\")",
              "validation": "required",
              "$formkit": "number",
              "min": 0
            },
            {
              "key": "women_priority",
              "name": "women_priority",
              "label": "$pgettext('sf23_priority', 'From priorities identified by women (yes/no)')",
              "validation": "required",
              "placeholder": "$gettext(\"Please select\")",
              "$formkit": "select",
              "options": "$ida(yesno)"
            },
            {
              "name": "cost_estimation",
              "label": "$pgettext('sf23_priority', 'Cost Estimation')",
              "validation": "required",
              "$formkit": "currency",
              "min": 0,
              "step": "0.01"
            }
          ],
          "id": "priorities",
          "name": "priorities",
          "$formkit": "repeater",
          "upControl": false,
          "downControl": false,
          "addLabel": "$pgettext(\"sf23_priority\", \"Add another priority\")",
          "removeLabel": "$gettext(\"Remove\")",
          "removeAction": "$repeaterRemoveAction",
          "sectionsSchema": {
            "remove": {
              "children": [
                {
                  "if": "$value.length > 1",
                  "$el": "button",
                  "attrs": {
                    "class": "disabled:hidden cursor-pointer flex items-center justify-center flex-row-reverse h-[50px] rounded-2.5xl px-4 w-full bg-white border-2 border-solid border-emerald-600 text-zinc-900 hover:bg-emerald-600 hover:text-white font-bold text-base",
                    "onClick": "$attrs.removeAction",
                    "data-index": "$index",
                    "data-repeaterid": "$id"
                  },
                  "children": [
                    "$attrs.removeLabel"
                  ]
                }
              ]
            }
          },
          "repeaterUniqueIdField": {
            "name": "uuid",
            "$formkit": "uuid",
            "readonly": true
          }
        },
        {
          "name": "submitted_from",
          "value": "data-collection",
          "$formkit": "hidden"
        }
      ],
      "id": "priorities",
      "$formkit": "group",
      "icon": "las la-list-alt",
      "title": "Priorities"
    }
  ],
  "name": "SF_2_3",
  "$formkit": "group"
}