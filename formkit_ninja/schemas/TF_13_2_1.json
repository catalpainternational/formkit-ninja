{
  "children": [
    {
      "children": [
        {
          "key": "district",
          "id": "district",
          "name": "district",
          "label": "$gettext(Municipality)",
          "validation": "required",
          "$formkit": "select",
          "options": "$getLocations()"
        },
        {
          "key": "administrative_post",
          "if": "$get(district).value",
          "id": "administrative_post",
          "name": "administrative_post",
          "label": "$gettext(\"Administrative Post\")",
          "validation": "required",
          "$formkit": "select",
          "options": "$getLocations($get(district).value)"
        },
        {
          "key": "date",
          "id": "date",
          "name": "date",
          "label": "$gettext(\"Date\")",
          "validation": "dateInPast",
          "$formkit": "datepicker",
          "calendarIcon": "calendar",
          "format": "DD/MM/YYYY",
          "nextIcon": "angleRight",
          "prevIcon": "angleLeft",
          "_currentDate": "$getCurrentDate",
          "sectionsSchema": {
            "day": {
              "children": [
                "$day.getDate()",
                {
                  "if": "$attrs._currentDate().day === $day.getDate()",
                  "children": [
                    {
                      "if": "$attrs._currentDate().month === $day.getMonth()",
                      "children": [
                        {
                          "if": "$attrs._currentDate().year === $day.getFullYear()",
                          "$el": "div",
                          "attrs": {
                            "class": "formkit-day-highlight"
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          },
          "valueFormat": "YYYY-MM-DD"
        },
        {
          "name": "month",
          "label": "$gettext(\"Month\")",
          "validation": "required",
          "placeholder": "$gettext(\"Select month\")",
          "$formkit": "select",
          "options": "$ida(month)"
        },
        {
          "name": "year",
          "label": "$gettext(\"Year\")",
          "validation": "required",
          "placeholder": "$gettext(\"Select year\")",
          "$formkit": "select",
          "options": "$ida(year)"
        }
      ],
      "id": "meetingInformation",
      "$formkit": "group",
      "icon": "las la-info-circle",
      "title": "Location and Reporting Period"
    },
    {
      "children": [
        {
          "children": [
            {
              "name": "uuid",
              "$formkit": "uuid",
              "readonly": true
            },
            {
              "children": [
                "$gettext(\"project\")",
                " ",
                {
                  "children": [
                    "$: ($index + 1)"
                  ],
                  "classes": "mx-1",
                  "$el": "span"
                }
              ],
              "$el": "div",
              "attrs": {
                "class": "rounded-full px-5 py-2 bg-zinc-400 text-lg font-bold mb-5"
              }
            },
            {
              "key": "$: (sukus + $index)",
              "id": "$: (sukus + $index)",
              "name": "suco",
              "label": "$gettext(\"Suku\")",
              "validation": "required",
              "placeholder": "$gettext(\"Select one\")",
              "$formkit": "select",
              "options": "$getLocations($get(district).value, $get(administrative_post).value)"
            },
            {
              "key": "$: (project + $index)",
              "if": "$ida(project, \"suku_id=\"+ $get(\"sukus\"+ $index).value).length>0",
              "id": "$: (project + $index)",
              "name": "project",
              "label": "$gettext(\"Project name\")",
              "validation": "required",
              "placeholder": "$gettext(\"Search project\")",
              "$formkit": "autocomplete",
              "options": "$ida(project, \"suku_id=\"+ $get(\"sukus\"+ $index).value)",
              "inputClass": "!pl-10",
              "prefixIcon": "search"
            },
            {
              "name": "total",
              "label": "Total (%)",
              "validation": "required",
              "placeholder": "$gettext(\"Please enter\")",
              "$formkit": "number",
              "max": 100,
              "min": 0
            }
          ],
          "id": "repeaterProjectProgress",
          "name": "repeaterProjectProgress",
          "$formkit": "repeater",
          "upControl": false,
          "downControl": false,
          "addLabel": "$gettext(\"Add project\")",
          "min": 1,
          "itemClass": "repeater-children-index",
          "itemsClass": "repeater",
          "removeLabel": "$gettext(\"Remove\")",
          "removeAction": "$repeaterRemoveAction",
          "sectionsSchema": {
            "remove": {
              "children": [
                {
                  "if": "$value.length > 1",
                  "$el": "button",
                  "attrs": {
                    "class": "disabled:hidden cursor-pointer flex items-center justify-center flex-row-reverse h-[50px] rounded-2.5xl px-4 w-full bg-white border-2 border-solid border-emerald-600 text-zinc-900 hover:bg-emerald-600 hover:text-white font-bold text-base",
                    "onClick": "$attrs.removeAction",
                    "data-index": "$index",
                    "data-repeaterid": "$id"
                  },
                  "children": [
                    "$attrs.removeLabel"
                  ]
                }
              ]
            }
          }
        }
      ],
      "id": "projectProgress",
      "$formkit": "group",
      "icon": "las la-users-cog",
      "title": "Project progress",
      "sectionTitle": "Project Progress"
    }
  ],
  "name": "TF_13_2_1",
  "$formkit": "group"
}