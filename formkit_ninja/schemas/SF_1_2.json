{
  "children": [
    {
      "children": [
        {
          "key": "district",
          "id": "district",
          "name": "district",
          "label": "$gettext(Municipality)",
          "validation": "required",
          "$formkit": "select",
          "options": "$getLocations()"
        },
        {
          "key": "administrative_post",
          "if": "$get(district).value",
          "id": "administrative_post",
          "name": "administrative_post",
          "label": "$gettext(\"Administrative Post\")",
          "validation": "required",
          "$formkit": "select",
          "options": "$getLocations($get(district).value)"
        },
        {
          "key": "suco",
          "if": "$get(administrative_post).value",
          "id": "suco",
          "name": "suco",
          "label": "$gettext(Suco)",
          "validation": "required",
          "$formkit": "select",
          "options": "$getLocations($get(district).value, $get(administrative_post).value)"
        },
        {
          "key": "aldeia",
          "if": "$get(suco).value",
          "id": "aldeia",
          "name": "aldeia",
          "label": "$gettext(Aldeia)",
          "validation": "required",
          "$formkit": "select",
          "options": "$getLocations($get(district).value, $get(administrative_post).value, $get(suco).value)"
        },
        {
          "key": "date",
          "id": "date",
          "name": "date",
          "label": "$gettext(\"Date\")",
          "validation": "dateInPast",
          "$formkit": "datepicker",
          "calendarIcon": "calendar",
          "format": "DD/MM/YYYY",
          "nextIcon": "angleRight",
          "prevIcon": "angleLeft",
          "_currentDate": "$getCurrentDate",
          "sectionsSchema": {
            "day": {
              "children": [
                "$day.getDate()",
                {
                  "if": "$attrs._currentDate().day === $day.getDate()",
                  "children": [
                    {
                      "if": "$attrs._currentDate().month === $day.getMonth()",
                      "children": [
                        {
                          "if": "$attrs._currentDate().year === $day.getFullYear()",
                          "$el": "div",
                          "attrs": {
                            "class": "formkit-day-highlight"
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          },
          "valueFormat": "YYYY-MM-DD"
        }
      ],
      "id": "location",
      "$formkit": "group",
      "icon": "las la-map-marked-alt",
      "title": "Location"
    },
    {
      "children": [
        {
          "key": "project_name",
          "id": "project_name",
          "name": "project_name",
          "label": "$gettext(\"Project name\")",
          "help": "",
          "validation": "",
          "placeholder": "$gettext(\"Please enter\")",
          "$formkit": "autocomplete",
          "options": "$ida(output)",
          "inputClass": "!pl-3"
        },
        {
          "name": "cost_estimation",
          "label": "$gettext(\"Cost estimation\")",
          "validation": "",
          "placeholder": "$gettext(\"Please enter\")",
          "$formkit": "currency",
          "min": 0
        }
      ],
      "id": "community",
      "$formkit": "group",
      "icon": "las la-address-book",
      "title": "Community contribution"
    }
  ],
  "name": "SF_1_2",
  "$formkit": "group"
}