       "name": "name",
       "placeholder": "$gettext(\"Please enter\")"
      },
      {
       "$formkit": "radio",
       "label": "$gettext(\"Gender\")",
       "name": "gender",
       "optionClass": "mr-6",
       "options": "$ida(gender)",
       "optionsClass": "flex flex-wrap"
      },
      {
       "$formkit": "radio",
       "label": "$gettext(\"Person with a disability\")",
       "name": "person_with_disability",
       "optionClass": "mr-6",
       "options": "$ida(yesno)",
       "optionsClass": "flex flex-wrap"
      },
      {
       "$formkit": "tel",
