      }
     ],
     "downControl": false,
     "id": "repeaterSukus",
     "itemClass": "repeater-children-index",
     "itemsClass": "repeater",
     "max": 2,
     "min": 1,
     "name": "repeaterSukus",
     "removeAction": "$repeaterRemoveAction",
     "removeLabel": "$gettext(\"Remove\")",
     "sectionsSchema": {
      "remove": {
